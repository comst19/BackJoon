package Silver;

import java.util.*;

public class S1_1389 {

	static int n,m;
	static boolean[] visit;
	static int[][] arr;
	
	public static void BFS(int f) {
		
		visit = new boolean[n+1];
		
		for(int i = 1; i <= n; i++) {
			if(arr[f][i] == 1) {
				visit[i] = true;
				arr[f][i]
			} 
		}

	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		m = sc.nextInt();
		
		arr = new int[n+1][+1];
		Arrays.fill(arr, Integer.MAX_VALUE);
		for(int i = 1; i <= n; i++) arr[i][i] = 0;
		
		for(int i = 0; i < m; i++) {
			int f1 = sc.nextInt();
			int f2 = sc.nextInt();
			arr[f1][f2] = 1;
			arr[f2][f1] = 1;
		}
		
		for(int i = 1; i <= n; i++) {
			BFS(i);
		}
	}

}
