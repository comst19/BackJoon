package Gold;

import java.util.*;

class Time implements Comparable<Time>{
	public int st, et;
	public Time(int st, int et) {
		this.st = st;
		this.et = et;
	}
	@Override
	public int compareTo(Time o) {
		// TODO Auto-generated method stub
		return this.et - o.et;
	}
	
	
}

public class G3_12764 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		ArrayList<Time> arr = new ArrayList<>();
		ArrayList<Integer> seat = new ArrayList<>();
		int n = sc.nextInt();
		for(int i = 0; i < n; i++) {
			int st = sc.nextInt();
			int et = sc.nextInt();
			arr.add(new Time(st,et));
		}
		
		Collections.sort(arr);
		
		int cnt = 0;
		for(Time cur : arr) {
			if(cnt == 0) {
				cnt++;
				seat.add(cur.et);
			}else {
				int i = 0;
				for(Integer t : seat) {
					if(cur.st > t) {
						seat.set(i, cur.et);
						System.out.println(seat.get(i));
						break;
					}
					i++;
				}
				cnt++;
				seat.add(cur.et);
				
			}
			
		}
		
	}

}
