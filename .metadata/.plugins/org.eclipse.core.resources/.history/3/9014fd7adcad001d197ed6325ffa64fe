package Silver;

import java.util.*;

public class S1_1080 {
	
	static int[][] map1;
	
	static void reverse(int y, int x) {
		for(int i = y; i <= y + 2; y++) {
			for(int j = x; j <= x + 2; j++) {
				map1[i][j] = 1 - map1[i][j];
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		map1 = new int[n][m];
		int[][] map2 = new int[n][m];
        for (int i = 0; i < n; i++) {
            String s = sc.next();
            for (int j = 0; j < m; j++) {
                map1[i][j] = s.charAt(j) - '0';
            }
        }
        for (int i = 0; i < n; i++) {
            String s = sc.next();
            for (int j = 0; j < m; j++) {
                map2[i][j] = s.charAt(j) - '0';
            }
        }
        
        int cnt = 0;
        for(int i = 0; i <= n-3; i++) {
        	for(int j = 0; j <= m-3; j++) {
        		if(map1[i][j] != map2[i][j]) {
        			cnt++;
        			reverse(i,j);
        		}
        	}
        }
    
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                if(map1[i][j] != map2[i][j]) {
                	System.out.println(-1);
                	System.exit(0);
                }
            }
        }
		
        System.out.println(cnt);
	}
	

}
