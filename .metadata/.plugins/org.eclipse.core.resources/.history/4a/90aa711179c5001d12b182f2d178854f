package Silver;

import java.util.*;

public class S2_1699 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		int[] dp = new int[n+1];
		int zz = 1;
		
		while(true) {
			if(zz*zz <= n) {
				dp[zz*zz] = 1;
			}else {
				break;
			}
			zz++;
		}
		
		for(int i = 2; i <= n; i++) {
			dp[i] = dp[i-1]+1;
		}
		
		System.out.println(dp[n]);
		
	}

}
