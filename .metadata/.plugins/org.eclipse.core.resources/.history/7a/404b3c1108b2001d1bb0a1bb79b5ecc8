package Silver;

import java.util.*;
import java.io.*;

public class S2_1406 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

//		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
//		String s = br.readLine();
//		int n = Integer.parseInt(br.readLine());
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		int n = sc.nextInt();
		
		Stack<Character> left = new Stack<>();
		Stack<Character> right = new Stack<>();
		
		for(int i = 0; i < s.length(); i++) left.push(s.charAt(i));
		
		for(int i = 0; i < n; i++) {
			char com = sc.next().charAt(0);
			if(com == 'P') {
				char c = sc.next().charAt(0); // P x - xëŠ” idx 2
				right.push(c);
			}else if(com == 'B') {
				left.pop();
			}else if(com == 'L' && !left.isEmpty()) {
				right.push(left.pop());
			}else if(com == 'D' && right.isEmpty()) {
				left.push(right.pop());
			}
		}
		while(!right.isEmpty()) right.push(left.pop());
		while(!left.isEmpty()) System.out.print(left.pop());
	}

}