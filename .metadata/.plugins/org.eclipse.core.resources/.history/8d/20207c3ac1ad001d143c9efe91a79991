package Silver;

import java.util.*;

public class S1_1080 {

	static int n,m;
	static char[][] map1, map2;
	static Scanner sc;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		sc = new Scanner(System.in);
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		map1 = new char[n][m];
		map2 = new char[n][m];
		
		dataSet(map1);
		dataSet(map2);
		System.out.println(map1[1][2]+map2[2][3]);
	}
	
	public static void dataSet(char[][] map) {
		for(int i = 0; i < n; i++) {
			String s = sc.next();
			for(int j = 0; j < m; j++) {
				map1[i][j] = s.charAt(j);
			}
		}
	}

}
