package Silver;

import java.util.*;

public class S1_1080 {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		int[][] map1 = new int[n][m];
		int[][] map2 = new int[n][m];
        for (int i = 0; i < n; i++) {
            String s = sc.next();
            for (int j = 0; j < m; j++) {
                map1[i][j] = s.charAt(j) - '0';
            }
        }
        for (int i = 0; i < n; i++) {
            String s = sc.next();
            for (int j = 0; j < m; j++) {
                map2[i][j] = s.charAt(j) - '0';
            }
        }
        
        int cnt = 0;
        for(int i = 0; i <= n-3; i++) {
        	for(int j = 0; j <= m-3; j++) {
        		if(map1[i][j] != map2[i][j]) {
        			for(int y = i; i <= i+2; i++) {
        				for(int x = j; j <= j+2; j++) {
        					if(map1[y][x] == 1) map1[y][x] = 0;
        					else map1[y][x] = 1;
        				}
        			}
        			cnt++;
        		}
        	}
        }
        
        boolean flag = true;
        
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                if(map1[i][j] != map2[i][j]) {
                	flag = false;
                	break;
                }
            }
            if(!flag) break;
        }
		
        System.out.println(cnt);
	}
	

}
