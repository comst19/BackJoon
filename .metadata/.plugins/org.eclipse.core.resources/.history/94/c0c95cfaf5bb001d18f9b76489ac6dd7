package Silver;

import java.util.*;

public class S1_1309 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[][] dp = new int[n+1][3];
		Arrays.fill(dp[1],1);
		
		for(int x : dp[1]) System.out.print(x + " ");
	}

}
