package Silver;

import java.util.*;

public class S1_11660 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		int[][] arr = new int[n+1][n+1];
		for(int i = 1; i <= n ; i++) {
			for(int j = 1; j <= n; j++) {
				arr[i][j] = arr[i-1][j] + arr[i][j-1] - arr[i-1][j-1] + sc.nextInt();
			}
		}
		
		for(int i = 1; i <= n ; i++) {
			for(int j = 1; j <= n; j++) {
				System.out.print(arr[i][j] + " ");
			}
			System.out.println();
		}
		
//		for(int i = 0; i < m; i++) {
//			int x1 = sc.nextInt();
//			int y1 = sc.nextInt();
//			int x2 = sc.nextInt();
//			int y2 = sc.nextInt();
//			int asnwer = arr[y2][x2] - arr[y2][x1-1] - arr[y1-1][x2] + arr[y1-1][x1-1];
//			System.out.println(asnwer);
//		}
	}

}
