package Silver;

import java.util.*;

public class S1_1325 {

	static int n,m;
	static ArrayList<ArrayList<Integer>> graph;
	
	public static int BFS(int start) {
		boolean[] visited = new boolean[n+1];
		Queue<Integer> q = new LinkedList<>();
		q.offer(start);
		visited[start] = true;
		int cnt = 1;
		while(!q.isEmpty()) {
			int cur = q.poll();
			for(int nCur : graph.get(cur)) {
				q.offer(nCur);
				cnt++;
			}
		}
		return cnt;
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		m = sc.nextInt();
		int[] computer = new int[n+1];
		Arrays.fill(computer, Integer.MIN_VALUE);
		graph = new ArrayList<>();
		for(int i = 0; i <= n; i++) graph.add(new ArrayList<Integer>());
		
		for(int i = 0; i < m; i++) {
			int c1 = sc.nextInt();
			int c2 = sc.nextInt();
			graph.get(c2).add(c1);
		}
		int max = Integer.MIN_VALUE;
		int num = 1;
		while(num <= n) {
			computer[num] = BFS(num);
			max = Math.max(max, computer[num]);
			num++;
		}
		
		for(int i = 1; i <= n; i++) {
			if(computer[i] == max) System.out.print(i + " ");
		}
		
	}

}
