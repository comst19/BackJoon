package Silver;

import java.util.*;

public class S2_1912 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] dp = new int[n+1];
		dp[1] = sc.nextInt();
		for(int i = 2; i <= n; i++) {
			int num = sc.nextInt();
			dp[i] = Math.max(num, dp[i-1] + num);
		}
		Arrays.sort(dp);
		System.out.println(dp[n]);
	}

}
